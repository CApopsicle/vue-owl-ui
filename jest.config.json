{
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "test/mock-loader.js",
    "\\.(s?css)$": "identity-obj-proxy"
  },
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "\\.s?css$"
  ],
  "setupFiles": [
    "./test/setup.js"
  ],
  "collectCoverageFrom": [
    "src/components/*/*.js"
  ],
  "testPathDirs": [
    "src/"
  ]
}
