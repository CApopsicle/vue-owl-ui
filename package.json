{
  "name": "@cepave/owl-ui",
  "version": "0.2.1",
  "description": "OWL UI Â· Component Design System - Cepave F2E",
  "main": "./dist/owl-ui",
  "scripts": {
    "start": "babel-node scripts/start",
    "test": "BABEL_DISABLE_CACHE=1 jest --coverage",
    "codecov": "curl -s https://codecov.io/bash | bash",
    "dev": "webpack-dev-server --hot --inline --config webpack/client --port 3032",
    "dev.ssr": "babel-node ./scripts/dev.ssr",
    "pack.client": "webpack --progress --colors --config webpack/client",
    "build": "npm run build.ui & npm run build.pages",
    "build.ui": "babel-node ./scripts/build.ui",
    "build.pages": "NODE_ENV=production npm run pack.client",
    "lint": "eslint src && stylelint src/**/*.scss --syntax scss",
    "deploy": "npm run publish & npm run publish.pages && babel-node ./scripts/gh-tags",
    "publish": "cd npm && npm publish",
    "publish.pages": "babel-node ./scripts/gh-pages-publish"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "\\.s?css$"
    ],
    "setupFiles": [
      "./test/setup.js"
    ],
    "collectCoverageFrom": [
      "src/components/*/index.js"
    ],
    "testPathDirs": [
      "src/"
    ]
  },
  "keywords": [
    "design",
    "system",
    "vue",
    "component",
    "ui",
    "owl"
  ],
  "author": "Cepave F2E Team",
  "license": "MIT",
  "dependencies": {
    "delegate-to": "^0.1.4",
    "owl-icons": "github:cepave-f2e/owl-icons",
    "vue": "^2.1.8"
  },
  "devDependencies": {
    "axios": "^0.15.3",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.3.0",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-preset-env": "^1.1.6",
    "babel-preset-stage-2": "^6.18.0",
    "cash-dom": "^1.3.5",
    "chalk": "^1.1.3",
    "clipboard": "^1.5.16",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.3.2",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^2.1.0",
    "gulp-uglify": "^2.0.0",
    "html-webpack-plugin": "^2.26.0",
    "isomorphic-style-loader": "^1.1.0",
    "jest-cli": "^18.1.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.escape": "^4.0.1",
    "mark-it-down": "^0.3.0",
    "node-sass": "^4.1.1",
    "normalize.css": "^5.0.0",
    "piping": "^1.0.0-rc.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.1.1",
    "shelljs": "^0.7.5",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.1",
    "url-loader": "^0.5.7",
    "vue-router": "^2.1.1",
    "vue-server-renderer": "^2.1.8",
    "webpack": "2.2.0-rc.3",
    "webpack-dev-server": "2.2.0-rc.0"
  },
  "repository": "https://github.com/cepave-f2e/vue-owl-ui.git",
  "bugs": {
    "url": "https://github.com/cepave-f2e/vue-owl-ui/issues"
  },
  "homepage": "https://github.com/cepave-f2e/vue-owl-ui#readme"
}
